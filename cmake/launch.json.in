  {
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      {
        "type": "cortex-debug",
        "request": "launch",
        "name": "J-Link",
        "cwd": "${workspaceRoot}",
        "armToolchainPath": "/opt/gcc-arm-none-eabi/bin/",
        "executable": "$<TARGET_FILE:@PROJECT@>",
        "serverpath": "JLinkGDBServerCLExe",
        "ipAddress": "@JLINK_IP@",
        "runToEntryPoint": "main",
        "servertype": "jlink",
        "showDevDebugOutput": "both",
        "device": "@DEVICE@",
        "interface": "swd",
        "svdFile": "${workspaceRoot}/src/hardware/platform/stm32l476xx.svd",
        "rttConfig": {
          "enabled": true,
          "address": "auto",
          "decoders": [
            {
              "label": "",
              "port": 0,
              "type": "console"
            }
          ]
        }
      },
      {
      "type": "cortex-debug",
      "request": "launch",
      "name": "Remote OpenOCD",
      "cwd": "${workspaceRoot}",
      "armToolchainPath": "/opt/gcc-arm-none-eabi/bin/",
      "executable": "$<TARGET_FILE:@PROJECT@>",
      "servertype": "external",
      "gdbTarget": "host.docker.internal:3333",
      "interface": "swd",
      "runToEntryPoint": "main",
      "device": "@DEVICE@",  
      "svdFile": "${workspaceRoot}/src/hardware/platform/stm32l476xx.svd",
      "rttConfig": {
        "enabled": true,
        "address": "auto",
        "decoders": [
          {
            "label": "",
            "port": 0,
            "type": "console"
          }
        ]
      },
      "showDevDebugOutput": "both"
    }
    ]
  }

